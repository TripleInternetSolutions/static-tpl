{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && node versionize.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && node versionize.js"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "copy-webpack-plugin": "^5.1.0",
    "cross-env": "^6.0.3",
    "imagemin-webpack-plugin": "^2.4.2",
    "jquery": "^3.4.1",
    "laravel-mix": "^5.0.0",
    "laravel-mix-bundle-analyzer": "^1.0.4",
    "laravel-mix-imagemin": "^1.0.3",
    "popper.js": "^1.16.0",
    "replace-in-files": "^2.0.3",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
